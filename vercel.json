{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": { "zeroConfig": true },
      "branches": ["mooncity"]
    },
    {
      "src": "package.json",
      "use": "@vercel/svelte",
      "config": { "zeroConfig": true },
      "branches": ["main"]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}